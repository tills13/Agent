{
    "manifest_version": 2,
    "name": "Agent",
    "version": "0.0.0",
    "options_page": "src/html/options.html",
    "background": {
        "scripts": [
            "src/scripts/config.js",
            "src/scripts/background.js"
        ]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "*://*/*",
        "webRequest",
        "webRequestBlocking",
        "storage"
    ]
}